<% if projects.present? %>
  <ul>
    <% projects.each do |project| %>
      <li>
        <h3><%= project.title %></h3>
        <p><%= project.description %></p>
        <p>Status: <%= project.status %></p>
        <%# Add more project attributes as needed %>
      </li>
    <% end %>
  </ul>
<% else %>
  <p>No projects found.</p>
<% end %>
